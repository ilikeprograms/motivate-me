{
  "manifest_version": 2,

  "name": "Motivate Me",
  "description": "Adds a motivational quote under website headings",
  "version": "1.0.3",

  "author": "Thomas Coleman <tomc2k3@googlemail.com>",

  "permissions": [
    "activeTab",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],

  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
   "128": "assets/icon128.png"
  },

  "background": {
    "page": "index.html",
    "persistent": false
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
