
{
  "manifest_version": 2,

  "name": "Motivate Me",
  "description": "Adds a motivational quote under website headings",
  "version": "1.0",

  // "browser_action": {
  //   "default_popup": "index.html"
  // },
  "permissions": [
    "activeTab",
    "tabs",
    "background",
    "http://*/*",
    "https://*/*"
  ],

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "background": {
    "page": "index.html",
    "persistent": false
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
